{
  "name": "chessboard-sense",
  "version": "0.1.0",
  "description": "A TypeScript library for chessboard functionality",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "typescript",
    "library",
    "chessboard"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "tsdown": "^0.2.0",
    "typescript": "^5.3.0"
  }
}
